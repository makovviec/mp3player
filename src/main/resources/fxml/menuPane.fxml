<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="pl.f2s.mp3player.controller.MenuPaneController">
    <children>
        <MenuBar AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                 AnchorPane.topAnchor="0.0" maxWidth="1.7976931348623157E308">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem fx:id="fileMenuItem" mnemonicParsing="false" text="Otwórz plik" />
                        <MenuItem fx:id="dirMenuItem" mnemonicParsing="false" text="Otwórz folder" />
                        <MenuItem fx:id="closeMenuItem" mnemonicParsing="false" text="Zamknij" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="O programie">
                    <items>
                        <MenuItem fx:id="aboutMenuItem" mnemonicParsing="false" text="About" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </children>

</AnchorPane>
